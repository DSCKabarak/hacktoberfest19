//Hello world in C language
#include<stdio.h>
void main()
{
printf("Hello World");
}

#include <stdio.h>
int main() {
   // printf() displays the string inside quotation
   printf("Hello, World!");
   return 0;
}  
//another way to print hello world


#include<stdio.h>
main()
{
printf("Hello World!");
}
